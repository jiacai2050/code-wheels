{
  "hotkey": "alt+cmd+space",
  "theme": "tinytacoteam/zazu-light-theme",
  "displayOn": "detect",
  "plugins": [
    "tinytacoteam/zazu-calculator",
    "tinytacoteam/zazu-file-finder",
    "tinytacoteam/zazu-template",
    "tinytacoteam/zazu-system",
    {
      "name": "tinytacoteam/zazu-clipboard",
      "variables": {
        "ClipboardKey": "cmd+alt+b",
        "ignoreImages": true,
        "size": 1000
      }
    },
    {
      "name": "tinytacoteam/zazu-fallback",
      "variables": {
        "rootSearches": [
          "google"
        ]
      }
    },
    "twang2218/zazu-tldr",
    {
      "name": "puyt/zazu-utime",
      "variables": {
        "timestampUnit": "milliseconds",
        "outputFormat": "{yyyy}-{MM}-{dd} {HH}:{mm}:{s}"
      }
    },
    {
      "name": "tinytacoteam/zazu-fallback",
      "variables": {
        "rootSearches": [
          "g",
          "bd",
          "duck",
          "tw",
          "v2ex",
          "wiki"
        ],
        "prefixSearches": {
          "bd": {
            "icon": "assets/baidu.png",
            "name": "Baidu",
            "url": "https://www.baidu.com/s?ie=utf-8&wd="
          },
          "tw": {
            "icon": "assets/twitter.png",
            "name": "Twitter",
            "url": "https://twitter.com/search?q="
          },
          "v2ex": {
            "icon": "https://v2ex.com/static/img/icon_rayps_64.png",
            "name": "V2EX",
            "url": "https://www.google.com/search?q=site:v2ex.com "
          },
          "g": {
            "icon": "assets/google.png",
            "name": "Google",
            "url": "https://www.google.com/search?q="
          }
        }
      }
    }
  ]
}
